[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/srinivasdevops7
[access "refs/for/refs/*"]
	read = group user/srinivasdevops7
	push = group user/srinivasdevops7
	label-Code-Review = -2..+2 group user/srinivasdevops7
	label-Verified = -1..+1 group user/srinivasdevops7
	rebase = group user/srinivasdevops7
	removeReviewer = group user/srinivasdevops7
	submit = group user/srinivasdevops7
[access "refs/heads/*"]
	read = group user/srinivasdevops7
	create = group user/srinivasdevops7
	push = +force group user/srinivasdevops7
	pushMerge = group user/srinivasdevops7
[access "refs/tags/*"]
	push = +force group user/srinivasdevops7
	pushTag = group user/srinivasdevops7
	pushSignedTag = group user/srinivasdevops7
